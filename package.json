{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run lint && webpack-dev-server --mode development --hot --progress --open --port 4005",
    "local:start": "npm run remove:config && npm run copy:config:local && npm run lint && webpack-dev-server --mode development --hot --progress --open --port 4004",
    "dev:start": "npm run remove:config && npm run copy:config:dev && npm run lint && webpack-dev-server --mode development --hot --progress --open --port 4005",
    "uat:start": "npm run remove:config && npm run copy:config:uat && npm run lint && webpack-dev-server --mode development --hot --progress --open --port 4006",
    "build": "rm -rf dist && webpack --mode production",
    "lint": "eslint --fix ./src",
    "build:start": "node express-start.js",
    "simulate:prod": "rm -rf dist && webpack --mode production && node express-start.js",
    "remove:config": "rm -rf config/config.json",
    "copy:config:dev": "cp -f src/config/config-dev.json src/config/config.json",
    "copy:config:dev-site": "cp -f src/config/config-dev-site.json src/config/config.json",
    "copy:config:uat": "cp -f src/config/config-uat.json src/config/config.json",
    "copy:config:local": "cp -f src/config/config-local.json src/config/config.json",
    "clean": "rm -rf node-modules && rm -rf package-lock.json && npm i"
  },
  "repository": {
    "type": "git",
